<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 缩放的 ImageView -->
    <declare-styleable name="ScaleImageView">
        <attr name="scaleRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="imageViewdimensRatio" format="string" />
    </declare-styleable>

    <declare-styleable name="RatioRelativeLayout">
        <attr name="viewGroupDimentsRatio" format="string" />
    </declare-styleable>


    <declare-styleable name="WheelMaskView">
        <attr name="wheelMaskLineColor" format="color" />
    </declare-styleable>
    <declare-styleable name="WheelItemView">
        <attr name="wheelTextColor" format="color" />
        <attr name="wheelTextSize" format="dimension" />
        <attr name="wheelSelectTextColor" format="color" />
        <attr name="wheelSelectTextSize" format="dimension" />
        <attr name="wheelShowCount" format="integer" />
        <attr name="wheelTotalOffsetX" format="dimension" />
        <attr name="wheelItemVerticalSpace" format="dimension" />
        <attr name="wheelRotationX" format="float" />
        <attr name="wheelVelocityUnits" format="integer" />
    </declare-styleable>

    <declare-styleable name="SwitchButton">
        <!-- 是否选中 -->
        <attr name="android:checked" />
        <!-- 是否禁用 -->
        <attr name="android:enabled" />
        <!--开启颜色-->
        <attr name="selectColor" format="color" />
        <!--关闭颜色-->
        <attr name="unSelectColor" format="color" />
        <!--开启状态按钮描边色-->
        <attr name="primaryDarkColor" format="color" />
        <!--关闭状态按钮描边色-->
        <attr name="offDarkColor" format="color" />
    </declare-styleable>

    <declare-styleable name="LinkDevView">
        <attr name="linkCircleCount" format="integer" />
        <attr name="linkDefRadiius" format="dimension" />
        <attr name="linkSelRadius" format="dimension" />
        <attr name="linkDefColor" format="color" />
        <attr name="linkSelcolor" format="color" />
        <attr name="duration" format="integer" />

        <attr name="errorCenterColor" format="color" />
        <attr name="errorColor" format="color" />
        <attr name="linkErrorResultColor" format="color" />

        <attr name="completeCenterColor" format="color" />
        <attr name="completeColor" format="color" />
        <attr name="linkCompleteResultColor" format="color" />
    </declare-styleable>

    <declare-styleable name="DownProgressView">
        <attr name="strokeWidth" format="dimension" />

        <attr name="defCircleColor" format="color" />
        <attr name="completeCircleColor" format="color" />
        <attr name="errorCircleColor" format="color" />

        <attr name="defCirclrStyle" format="enum">
            <enum name="FILL" value="0" />
            <enum name="STROKE" value="1" />
        </attr>
        <attr name="completeCirclrStyle" format="enum">
            <enum name="FILL" value="0" />
            <enum name="STROKE" value="1" />
        </attr>
        <attr name="errorCirclrStyle" format="enum">
            <enum name="FILL" value="0" />
            <enum name="STROKE" value="1" />
        </attr>

        <attr name="maxProgress" format="integer" />
        <attr name="downProgress" format="integer" />
        <attr name="progessSize" format="dimension" />
        <attr name="progressColor" format="color" />
        <attr name="progressTextColor" format="color" />


        <attr name="completeResultColor" format="color" />
        <attr name="errorResultColor" format="color" />
    </declare-styleable>


    <declare-styleable name="WaveBall">
        <attr name="ball_color" format="color" />
        <attr name="wave_color" format="color" />
        <attr name="wave_height" format="integer" />
        <attr name="ball_stork_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="DegreeView">

        <attr name="cat_progress_bg_color" format="color" />
        <attr name="cat_progress_color" format="color" />
        <attr name="cat_slide_color" format="color" />
        <attr name="cat_progress_text_color" format="color" />

        <attr name="cat_progress_text_size" format="dimension" />
        <attr name="cat_slide_radius" format="dimension" />
        <attr name="cat_slide_max_radius" format="dimension" />
        <attr name="cat_progress_height" format="dimension" />

        <attr name="cat_min_progress" format="integer" />
        <attr name="cat_max_progress" format="integer" />
        <attr name="cat_current_progress" format="integer" />

        <attr name="cat_progress_percent" format="boolean" />

    </declare-styleable>

    <declare-styleable name="VerProgressBar">
        <attr name="vp_slide_color" format="color" />
        <attr name="vp_slide_radius" format="dimension" />
        <attr name="vp_slide_touch_radius" format="dimension" />
        <attr name="vp_slide_shadow_color" format="color" />

        <attr name="vp_pregress_bg_color" format="color" />
        <attr name="vp_progress_color" format="color" />
        <attr name="vp_progress_width" format="dimension" />

        <attr name="vp_max_progress" format="integer" />
        <attr name="vp_progress" format="integer" />

        <attr name="vp_animator_time" format="integer" />
    </declare-styleable>


    <declare-styleable name="TickView">

        <attr name="tiv_uncheck_color" format="color" />
        <attr name="tiv_stroken_width" format="dimension" />
        <attr name="tiv_check_mark_stroken_width" format="dimension" />

        <attr name="tiv_check_color" format="color" />
        <attr name="tiv_select_check_mark_color" format="color" />

        <attr name="tiv_animal_time" format="integer" />

        <attr name="tiv_radius" format="dimension" />
        <attr name="tiv_scale_factor" format="dimension" />

    </declare-styleable>

    <declare-styleable name="DropLoadView">

        <attr name="dlv_radius" format="dimension" />
        <attr name="dlv_rotate_angle" format="float" />
        <attr name="dlv_drop_time" format="integer" />
        <attr name="dlv_circle_color" format="color" />
        <attr name="dlv_rect_color" format="color" />
        <attr name="dlv_triangles_color" format="color" />

    </declare-styleable>

<!--    ///////////////////////////////////////////-->
    <declare-styleable name="PSTextView">

        <!-- 描边 宽度 0为填充-->
        <attr name="ps_border_width" format="dimension" />
        <!-- 描边 颜色-->
        <attr name="ps_border_color" format="color" />
        <!-- 填充 颜色-->
        <attr name="ps_background_color" format="color" />
        <!-- 圆角 -->
        <attr name="ps_radius" format="dimension" />
        <!-- 左上圆角-->
        <attr name="ps_top_left_radius" format="dimension" />
        <!-- 右上圆角-->
        <attr name="ps_top_right_radius" format="dimension" />
        <!-- 左下圆角-->
        <attr name="ps_bottom_left_radius" format="dimension" />
        <!-- 右下圆角-->
        <attr name="ps_bottom_right_radius" format="dimension" />
        <!-- focus 颜色-->
        <attr name="ps_focus_background_color" format="color" />
        <!-- disabled 颜色-->
        <attr name="ps_disable_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="PSButton">

        <!-- 描边 宽度 0为填充-->
        <attr name="ps_btn_border_width" format="dimension" />
        <!-- 描边 颜色-->
        <attr name="ps_btn_border_color" format="color" />
        <!-- 填充 颜色-->
        <attr name="ps_btn_background_color" format="color" />
        <!-- 圆角 -->
        <attr name="ps_btn_radius" format="dimension" />
        <!-- 左上圆角-->
        <attr name="ps_btn_top_left_radius" format="dimension" />
        <!-- 右上圆角-->
        <attr name="ps_btn_top_right_radius" format="dimension" />
        <!-- 左下圆角-->
        <attr name="ps_btn_bottom_left_radius" format="dimension" />
        <!-- 右下圆角-->
        <attr name="ps_btn_bottom_right_radius" format="dimension" />
        <!-- focus 颜色-->
        <attr name="ps_btn_focus_background_color" format="color" />
        <!-- disabled 颜色-->
        <attr name="ps_btn_disable_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <!-- 标题-->
        <attr name="ps_title" format="string" />
        <!-- 背景颜色-->
        <attr name="ps_title_background_color" format="color" />
        <!-- 右边文字-->
        <attr name="ps_right_text" format="string" />
        <!-- 右边按钮-->
        <attr name="ps_right_res" format="reference" />
        <!-- 返回键-->
        <attr name="ps_back_res" format="reference" />
        <!-- 右边文字颜色-->
        <attr name="ps_right_text_color" format="color" />
        <!-- 标题文字颜色-->
        <attr name="ps_title_text_color" format="color" />
        <!-- 标题文字大小-->
        <attr name="ps_title_text_size" format="dimension" />
        <!-- 右边文字大小-->
        <attr name="ps_right_text_size" format="dimension" />
        <!-- 是否需要返回键-->
        <attr name="ps_need_back" format="boolean" />
        <!-- 是否需要下间隔线-->
        <attr name="ps_need_line" format="boolean" />
        <!-- 间隔线颜色-->
        <attr name="ps_line_color" format="color" />
        <!-- 间隔线高度-->
        <attr name="ps_line_height" format="dimension" />

    </declare-styleable>

    <!-- ScaleView -->
    <declare-styleable name="ScaleView">
        <!-- 刻度颜色-->
        <attr name="h_lineColor" format="color" />
        <!-- 背景颜色-->
        <attr name="h_backgroundColor" format="color" />
        <!-- 数据颜色-->
        <attr name="h_dataColor" format="color" />
        <!-- 刻度线宽度-->
        <attr name="h_lineWidth" format="dimension" />
        <!-- lineHeight-->
        <attr name="h_lineHeight" format="dimension" />
        <!-- 分隔-->
        <attr name="h_lineSpace" format="integer" />
        <!-- 是否需要刻度-->
        <attr name="h_needLine" format="boolean" />
        <!-- 是否需要文字-->
        <attr name="h_needText" format="boolean" />
        <!-- 文字大小-->
        <attr name="h_textSize" format="dimension" />
        <!-- 左右边距-->
        <attr name="h_margin" format="dimension" />
    </declare-styleable>
</resources>
